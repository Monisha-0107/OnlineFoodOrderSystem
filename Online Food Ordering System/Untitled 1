create table  FOODORDER_TB(ORDERID VARCHAR2(12 BYTE) PRIMARY KEY,CUSTOMERNAME VARCHAR2(50 BYTE) Not null,FOODITEM VARCHAR2(50 BYTE) Not null ,ORDER_DATE DATE not null,QUANTITY NUMBER,PRICE NUMBER,REMARKS VARCHAR2(100 BYTE));

create sequence FOODORDER_SEQ STARTS WITH 10,MAX VALUE 99, INCREMENT BY 1;
